CREATE KEYSPACE IF NOT EXISTS eligibility_streams WITH replication = {'class':'SimpleStrategy', 'replication_factor':1};
CREATE TABLE IF NOT EXISTS eligibility_streams.membership(individual_identifier TEXT, membership_identifier TEXT, l_cov_prdt_dt BLOB, l_cov_prdt_dt_schema_id TEXT, l_hlt_srv_dt BLOB, l_hlt_srv_dt_schema_id TEXT, l_finc_acct BLOB, l_finc_acct_schema_id TEXT, cnsm_cov_cust_defn_fld BLOB, cnsm_cov_cust_defn_fld_schema_id TEXT, PRIMARY KEY(individual_identifier, membership_identifier));
CREATE TABLE IF NOT EXISTS eligibility_streams.mgrt_src_id_xref(  hst_lgcy_src_id TEXT,   hst_src_cd TEXT,   eff_dt TEXT,  canc_dt TEXT,  hst_cnsm_id INT,  hst_partn_nbr INT,   mgrt_cnsm_id INT,   mgrt_partn_nbr INT,   mgrt_src_cd TEXT,   mgrt_lgcy_src_id TEXT,   mgrt_lgcy_sbscr_id TEXT,   mgrt_lgcy_pol_nbr TEXT,   hst_xref_id_partn INT,  hst_cdb_xref_id INT,   mgrt_xref_id_partn INT,  mgrt_cdb_xref_id INT,  row_user_id TEXT,  row_tmstmp TEXT, row_sts_cd TEXT,  PRIMARY KEY (hst_lgcy_src_id, hst_src_cd, eff_dt)) WITH CLUSTERING ORDER BY (hst_src_cd asc, eff_dt desc);
CREATE TABLE IF NOT EXISTS eligibility_streams.schemas(id TEXT, subject TEXT, definition TEXT, PRIMARY KEY(id));
CREATE KEYSPACE IF NOT EXISTS encrypted_eligibility_streams WITH replication = {'class':'SimpleStrategy', 'replication_factor':1};
CREATE TABLE IF NOT EXISTS encrypted_eligibility_streams.membership(individual_identifier BLOB, membership_identifier BLOB, l_cov_prdt_dt BLOB, l_cov_prdt_dt_schema_id BLOB, l_hlt_srv_dt BLOB, l_hlt_srv_dt_schema_id BLOB, l_finc_acct BLOB, l_finc_acct_schema_id BLOB, cnsm_cov_cust_defn_fld BLOB, cnsm_cov_cust_defn_fld_schema_id BLOB, PRIMARY KEY(individual_identifier, membership_identifier));
CREATE TABLE IF NOT EXISTS encrypted_eligibility_streams.mgrt_src_id_xref(  hst_lgcy_src_id BLOB,   hst_src_cd BLOB,   eff_dt BLOB,  canc_dt BLOB,  hst_cnsm_id BLOB,  hst_partn_nbr BLOB,   mgrt_cnsm_id BLOB,   mgrt_partn_nbr BLOB,   mgrt_src_cd BLOB,   mgrt_lgcy_src_id BLOB,   mgrt_lgcy_sbscr_id BLOB,   mgrt_lgcy_pol_nbr BLOB,   hst_xref_id_partn BLOB,  hst_cdb_xref_id BLOB,   mgrt_xref_id_partn BLOB,  mgrt_cdb_xref_id BLOB,  row_user_id BLOB,  row_tmstmp BLOB, row_sts_cd BLOB,  PRIMARY KEY (hst_lgcy_src_id, hst_src_cd, eff_dt)) WITH CLUSTERING ORDER BY (hst_src_cd asc, eff_dt desc);
CREATE TABLE IF NOT EXISTS encrypted_eligibility_streams.schemas(id TEXT, subject TEXT, definition TEXT, PRIMARY KEY(id));
